{
  "disabled": false,
  "bindings": [
    {
      "type": "httpTrigger",
      "name": "req",
      "direction": "in",
      "dataType": "binary",
      "methods": ["post"],
      "route": "pvt/adhoc/upload"
    },
    {
      "type": "http",
      "direction": "out",
      "name": "response"
    },
    {
      "name": "storage",
      "type": "blob",
      "path": "%ADHOC_CONTAINER%/{sessionId}###{filename}###{guid}",
      "direction": "out",
      "connection": "STORAGECONNECTOR"
    }
  ]
}
